{
  "name": "react-router-wizard",
  "version": "0.1.0",
  "description": "A React container for assembling components into a wizard interface.",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack",
    "watch": "watch webpack ./lib",
    "test": "jest",
    "deploy": "npm run build && gh-pages -d example"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanconway/react-wizard"
  },
  "keywords": [
    "react",
    "tags",
    "input",
    "component",
    "javascript",
    "react-component"
  ],
  "author": "Jonathan Conway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanconway/react-wizard/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/jonathanconway/react-wizard",
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "gh-pages": "^1.0.0",
    "react-test-renderer": "^15.5.4",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.2",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.9",
    "@types/react-router-dom": "^4.0.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.0.9",
    "react-router-dom": "^4.0.9",
    "webpack": "^2.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$"
  }
}
